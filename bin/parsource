#!/usr/bin/env python
from parsource import Parser
from parsource.transform import TreeExtractor
import sys
with open(sys.argv[1], "rt") as f:
	extractor = TreeExtractor()
	for error in extractor.process(Parser().parse(f.read())):
		print ("ERROR", error)
	print (extractor.value.toTDoc())
# EOF - vim: syn=python ts=4 sw=4 noet
